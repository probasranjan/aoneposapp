<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
<Button android:text="BACK" android:id="@+id/dlg_btnBack" android:layout_width="fill_parent" android:layout_height="40sp"></Button>
<Button android:text="Get Challenge" android:id="@+id/dlg_btnGetChallenge" android:layout_width="fill_parent" android:layout_height="40sp"></Button>
<Button android:text="Update Firmware" android:id="@+id/dlg_btnUpdateFirmware" android:layout_width="fill_parent" android:layout_height="40sp"></Button>
<Button android:text="Get Version" android:id="@+id/dlg_btnGetVersion" android:layout_width="fill_parent" android:layout_height="40sp"></Button>
<Button android:text="Get Setting" android:id="@+id/dlg_btnGetSetting" android:layout_width="fill_parent" android:layout_height="40sp"></Button>
<Button android:text="Get Serial Number" android:id="@+id/dlg_btnGetSerialNumber" android:layout_width="fill_parent" android:layout_height="40sp"></Button>
<Button android:text="Turn on TEDS" android:id="@+id/dlg_btnTurnOnTEDS" android:layout_width="fill_parent" android:layout_height="40sp"></Button>
<Button android:text="Turn on AES" android:id="@+id/dlg_btnTurnOnAES" android:layout_width="fill_parent" android:layout_height="40sp"></Button>
